constructor TWaveFormSoundData.Create(wave: EWaveSound);
begin
  FWaveSound:=wave;
end;

function TWaveFormSoundData.SampleAt(var info: TWaveFormInfo; frequency: single): single;
VAR p:psingle;
    offset:integer;
begin
  with WaveData[FWaveSound] do
  begin
    p:=data;
    offset:=round(info.Position);
    offset:= min(offset,Size-1);
    inc(p,offset);
    result:=p^;
    info.Position:=info.Position+frequency/BaseFrequency;
    while (info.Position>=Size) do
       info.Position:=info.Position-Size;
  end;
end;

